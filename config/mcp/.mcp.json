{
  "mcpServers" : {
    "github" : {
      "description" : "Integração com GitHub",
      "command" : "npx @modelcontextprotocol/server-github",
      "priority" : 2,
      "enabled" : false,
      "env" : {
        "GITHUB_PERSONAL_ACCESS_TOKEN" : "${GITHUB_TOKEN}"
      },
      "args" : [ ]
    },
    "memory" : {
      "description" : "Armazenamento temporário em memória",
      "command" : "npx @modelcontextprotocol/server-memory",
      "priority" : 1,
      "enabled" : true,
      "env" : { },
      "args" : [ ]
    },
    "weather-nws" : {
      "description" : "Previsões meteorológicas via NWS",
      "command" : "npx @h1deya/mcp-server-weather",
      "priority" : 1,
      "enabled" : true,
      "env" : { },
      "args" : [ ]
    },
    "filesystem" : {
      "description" : "Sistema de arquivos - Documents",
      "command" : "npx @modelcontextprotocol/server-filesystem",
      "priority" : 3,
      "enabled" : true,
      "env" : { },
      "args" : [ "./documents" ]
    }
  }
}